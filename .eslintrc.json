{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes" : [
            "error",
            "double",
            { 
                "avoidEscape": true,
                "allowTemplateLiterals": true
            }
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-const-assign" : "error",
        "no-dupe-else-if" : "error",
        "no-duplicate-imports": "error",
        "no-unreachable": "error",
        "no-unused-vars": "error",
        "no-use-before-define":"error"
    }
}
